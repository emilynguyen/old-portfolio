<!doctype html>
<html>
<head>
    <!-- Meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{{title}}</title>
    <meta name="description" content="{{description}}" />
    <meta property="og:title" content="{{title}}">
    <meta property="og:description" content="{{description}}">
    {{#if icon}}
      <meta property="og:image" content="{{icon}}">
    {{/if}}

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-113482074-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());
        gtag('config', 'UA-113482074-1');
    </script>

    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="180x180" href="favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="favicons/favicon-16x16.png">
    <link rel="manifest" href="favicons/manifest.json">
    <link rel="mask-icon" href="favicons/safari-pinned-tab.svg" color="#ef675e">
    <meta name="apple-mobile-web-app-title" content="Emily Nguyen">
    <meta name="application-name" content="Emily Nguyen">
    <meta name="theme-color" content="#4a565c">

    <!-- Preload fonts -->
    <link rel="preload" href="/fonts/franklin-gothic/franklingothic-book-webfont.woff" as="font" crossorigin="anonymous" />
    <link rel="preload" href="/fonts/franklin-gothic/franklingothic-book-webfont.woff2" as="font" crossorigin="anonymous" />
    <link rel="preload" href="/fonts/franklin-gothic/franklingothic-bookit-webfont.woff" as="font" crossorigin="anonymous" />
    <link rel="preload" href="/fonts/franklin-gothic/franklingothic-bookit-webfont.woff2" as="font" crossorigin="anonymous" />
    <link rel="preload" href="/fonts/hk-grotesk/hkgrotesk-regular-webfont.woff" as="font" crossorigin="anonymous" />
    <link rel="preload" href="/fonts/hk-grotesk/hkgrotesk-regular-webfont.woff2" as="font" crossorigin="anonymous" />

    <!-- main.css -->
    <link href="/css/main.css" rel="stylesheet" type="text/css">
    <!-- jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <!-- jQuery local fallback -->
    <script>
    if (typeof jQuery == 'undefined') {
            document.write(unescape("%3Cscript src='js/jquery-3.3.1.min.js' type='text/javascript'%3E%3C/script%3E"));
        }
    </script>
    <base href="/">
</head>

<body>
	<div class="body-wrapper">

		<div id="header">
		    <div class="container">

		        <button class="hamburger hamburger--spin" type="button">
		          <span class="hamburger-box">
		            <span class="hamburger-inner"></span>
		          </span>
		        </button> <!-- .hamburger -->

		        <div class="menu-overlay">
		            <div class="menu-container v-center">
		                <ul class="menu">
		                    <li><h1><a href="/">Work</a></h1>
		                        <div class="menu-shortcuts">
		                        	{{#with data}}
		                            {{#each projects}}
		                            <h2><a href="/{{url}}">{{title}}</a></h2>
		                            {{/each}}
		                            {{/with}}
		                        </div> <!-- .menu-shortcuts -->
		                    </li>
		                    <li><h1><a href="/about">About</a></h1></li>
		                    <li><h1><a href="https://drive.google.com/file/d/0B6dEoGhrXuYId0g0Nm5BR1ZYbTg/view" target="_blank">Resume</a></h1></li>
		                </ul> <!-- .menu -->
		            </div> <!-- .menu-container -->
		        </div> <!-- .menu-overlay -->

		    </div> <!-- .container -->
		</div> <!-- #header -->

    	{{{body}}}

	</div> <!-- .body-wrapper -->

	<div id="footer">
	    <div class="container h-space-beteen v-cnter">
	        <div class="copyright">
	            <h2>Emily Nguyen Â© 2018</h2>
	        </div> <!-- .copyright -->
	        <div class="social-media">
	            <ul>
	                <li><a href="https://drive.google.com/file/d/0B6dEoGhrXuYId0g0Nm5BR1ZYbTg/view" target="_blank" title="Resume"><img src="images/icons/resume.svg"></a></li>
	                <li class="email"><a href="mailto:emilymylinh@gmail.com" title="Email"><img src="images/icons/mail.svg"></a></li>
	                <li><a href="https://www.linkedin.com/in/emilymnguyen/" target="_blank" title="LinkedIn"><img src="images/icons/linkedin.svg"></a></li>
	                <li class="github"><a href="https://github.com/emnguyen" target="_blank" title="Github"><img src="images/icons/github.svg"></a></li>
	                <li><a href="https://dribbble.com/emnguyen" target="_blank" title="Dribbble"><img src="images/icons/dribbble.svg"></a></li>
	            </ul>
	        </div> <!-- .social-media -->
	    </div> <!-- .container -->
	</div> <!-- #footer -->

	<!-- main.js -->
	<script type="text/javascript" src="/js/main.js"></script>
</body>
</html>
